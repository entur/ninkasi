    "@mui/x-date-pickers": "8.10.0",
    "@mui/x-date-pickers-pro": "8.10.0",
    "axios": "1.11.0",
    "chai": "4.5.0",
    "chart.js": "2.9.4",
    "classnames": "2.5.1",
    "clipboard-js": "0.3.6",
    "connected-react-router": "6.9.3",
    "file-saver": "2.0.5",
    "halogenium": "2.3.0",
    "history": "4.10.1",
