    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "lint-staged": "16.2.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1",
