    "@mui/icons-material": "7.3.2",
    "@mui/material": "7.3.2",
    "@mui/x-date-pickers": "8.11.3",
    "axios": "1.12.2",
    "chai": "5.3.3",
    "clipboard-js": "0.3.6",
    "file-saver": "2.0.5",
    "history": "4.10.1",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "oidc-client-ts": "3.3.0",
    "prop-types": "15.8.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-icons": "5.5.0",
    "react-oidc-context": "3.3.0",
    "react-redux": "9.2.0",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "redux": "5.0.1",
    "redux-devtools-extension": "2.13.9",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.4.2",
    "sass": "1.93.1",
    "uuid": "3.4.0"
