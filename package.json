    "@material-ui/core": "4.8.3",
    "@material-ui/icons": "4.5.1",
    "@material-ui/lab": "4.0.0-alpha.39",
    "axios": "0.26.1",
    "bogu": "git://github.com/entur/bogu.git#v1.3.2",
    "chai": "4.3.6",
    "chart.js": "2.9.4",
    "classnames": "2.3.1",
    "clipboard-js": "0.3.6",
    "connected-react-router": "6.9.2",
    "convict": "4.4.1",
    "express": "4.17.3",
    "express-history-api-fallback": "2.2.1",
    "file-saver": "2.0.5",
    "halogenium": "2.3.0",
    "history": "4.10.1",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "lodash": "4.17.21",
    "material-ui": "0.20.0",
    "moment": "2.29.1",
    "muicss": "0.10.3",
    "node-sass": "4.14.1",
    "prettier": "1.19.1",
    "prop-types": "15.7.2",
    "rc-progress": "2.5.2",
    "react": "16.14.0",
    "react-chartjs-2": "2.8.0",
    "react-dom": "16.14.0",
    "react-dropzone": "4.1.3",
    "react-icons": "2.2.7",
    "react-notification": "6.8.5",
    "react-notification-system": "0.2.17",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-scripts": "3.2.0",
    "react-select": "2.4.4",
    "redux": "4.0.4",
    "redux-devtools-extension": "2.13.9",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "styled-components": "4.4.1",
    "uuid": "3.3.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
