  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.6.4",
    "@vitejs/plugin-react": "5.0.0",
    "@vitest/coverage-v8": "3.2.4",
    "husky": "9.1.7",
    "jsdom": "26.1.0",
    "lint-staged": "16.1.5",
    "prettier": "3.6.2",
    "vite": "7.1.1",
    "vitest": "3.2.4"
  }
